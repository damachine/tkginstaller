[0.00062, "o", "\r\u001b[2d\u001b(B\u001b[0;1m\u001b[34m# nvidia-all config file\r\u001b[5d#### OPTIONS ####\r\u001b[7d# External config file to use - If the given file exists in path, it will override default config (customization.cfg) - Default is ~/.config/frogminer/nvidia-all.cfg\r\u001b[8d\u001b(B\u001b[0m\u001b[36m_EXT_CONFIG_PATH\u001b[31m=\u001b[39m\u001b(B\u001b[m~/.config/frogminer/nvidia-all.cfg\r\u001b[10d\u001b(B\u001b[0;1m\u001b[34m# Custom compiler root dirs - For non-dkms builds (see readme to use this with dkms) - Leave empty to use system compilers\r\u001b[11d# ! If your kernel was built with mostlyportable GCC, you need to use the exact same build here or module compilation will fail !\r\u001b[12d# Example: CUSTOM_GCC_PATH=\"/home/frog/PKGBUILDS/mostlyportable-gcc/gcc-mostlyportable-9.2.0\"\r\u001b[13d\u001b(B\u001b[0m\u001b[36mCUSTOM_GCC_PATH\u001b[31m=\u001b[33m\"\"\r\u001b[15d\u001b(B\u001b[0;1m\u001b[34m# Set to \"true\" to add driver series versioning to package name. For repo owners wanting to store multiple series at once.\r\u001b[16d\u001b(B\u001b[0m\u001b[36m_series_in_pkgname\u001b[31m=\u001b[33m\"\"\r\u001b[18d\u001b(B\u001b[0;1m\u001b[34m# Allows enforcing kernel patches application for a target kernel, independently of currently installed ones (example: \"5.5\")\r\u001b[19d# You typically don't want to use this as a user. The proper way it to install your kernel(s), then run makepkg against nvidia-all's PKGBUILD to get things autodetected.\r\u001b[20d\u001b(B\u001b[0m\u001b[36m_kerneloverride\u001b[31m=\u001b[33m\"\"\r\u001b[22d\u001b(B\u001b[0;1m\u001b[34m# Put the built packages in a versioned dir in the same folder as the nvidia-all PKGBUILD on exit - Will fail to install if running makepkg with -i option\r\u001b[23d\u001b(B\u001b[0m\u001b[36m_local_package_storing\u001b[31m=\u001b[33m\"false\"\r\u001b[25d\u001b(B\u001b[0;1m\u001b[34m# If you want to ignore some packages, set them to \"false\". Default is all set to \"true\" for a complete installation.\r\u001b[26d\u001b(B\u001b[0m\u001b[36m_lib32\u001b[31m=\u001b[33m\"true\"\r\u001b[27d\u001b[36m_opencl\u001b[31m=\u001b[33m\"true\"\r\u001b[28d\u001b[36m_nvsettings\u001b[31m=\u001b[33m\"true\"\r\u001b[30d\u001b(B\u001b[0;1m\u001b[34m# Set _eglwayland to \"true\" to create egl-wayland package using Nvidia's tarball - conflicts with Arch's egl-wayland package\r\u001b[31d# Set _eglwayland to \"false\" to skip egl-wayland package creation and not depend on the Arch provided egl-wayland package\r\u001b[32d# Set _eglwayland to \"external\" to use the Arch provided egl-wayland package (default)\r\u001b[33d\u001b(B\u001b[0m\u001b[36m_eglwayland\u001b[31m=\u001b[33m\"external\"\r\u001b[35d\u001b(B\u001b[0;1m\u001b[34m# Set _eglgbm to \"true\" to add egl-gbm binaries to the egl-wayland package using Nvidia's tarball - conflicts with Arch's egl-gbm package\r\u001b[36d# Set _eglgbm to \"false\" to skip Nvidia's tarball egl-gbm binaries and not depend on the Arch provided egl-gbm package\r\u001b[37d# Set _eglgbm to \"external\" to use the Arch provided egl-gbm package (default)\r\u001b[38d# Depends on _eglwayland=\"external\"\r\u001b[39d\u001b(B\u001b[0m\u001b[36m_eglgbm\u001b[31m=\u001b[33m\"external\"\r\u001b[41d\u001b(B\u001b[0;1m\u001b[34m# Either \"vulkandev\" for vulkan developer drivers or \"regular\" for all others\r\u001b[42d\u001b(B\u001b[0m\u001b[36m_driver_branch\u001b[31m=\u001b[33m\"\"\r\u001b[44d\u001b(B\u001b[0;1m\u001b[34m# Desired driver version - !! needs to be available from the selected branch above !!\r\u001b[45d"]
[0.000003, "o", "# Set to \"latest\" to use the latest driver on the selected branch.\r\u001b[46d"]
[0.000005, "o", "\u001b(B\u001b[0m\u001b[36m_driver_version\u001b[31m=\u001b[33m\"\"\r\u001b[48d"]
[0.000005, "o", "\u001b(B\u001b[0;1m\u001b[34m# Set to \"true\" to build open source modules if available, or leave empty to get a prompt\r\u001b[49d# Available for 515.43.04 beta or newer only\r\u001b[50d"]
[0.000006, "o", "\u001b(B\u001b[0m\u001b[36m_open_source_modules\u001b[31m=\u001b[33m\"\"\r\u001b[52d"]
[0.000001, "o", "\u001b(B\u001b[0;1m\u001b[34m# Set to \"true\" to blacklist nouveau. may be needed early kms on nvidia.\r\u001b[53d"]
